{
  "name": "softeo-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.17.2"
  },
  "scripts": {
    "build": "npm run db:start && tsc",
    "start": "tsc && node ./dist/index.js",
    "dev": "npm run db:start && tsc && npm run db:seed && nodemon ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --name init --schema ./src/prisma/schema.prisma",
    "db:start": "npm run prisma:migrate && npm run prisma:generate",
    "db:start:dev": "npm run prisma:migrate:dev && npm run prisma:generate",
    "db:seed": "ts-node ./src/prisma/seeds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.13",
    "nodemon": "^2.0.19",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1"
  }
}
